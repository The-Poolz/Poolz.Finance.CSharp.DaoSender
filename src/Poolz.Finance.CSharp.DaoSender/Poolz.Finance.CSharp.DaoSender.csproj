<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	  <TargetFramework>net8.0</TargetFramework>
	  <ImplicitUsings>enable</ImplicitUsings>
	  <Nullable>enable</Nullable>
	  <PackageLicenseFile>LICENSE</PackageLicenseFile>
	  <IncludeSymbols>True</IncludeSymbols>
	  <SymbolPackageFormat>snupkg</SymbolPackageFormat>
	  <PackageId>Poolz.Finance.CSharp.DaoSender</PackageId>
	  <Title>Auto-generated C# GraphQL client for DaoSender of The-Poolz company.</Title>
	  <Version>1.0.0</Version>
	  <Authors>ArdenHide, Lomet</Authors>
	  <Company>The-Poolz</Company>
	  <Product>Poolz.Finance.CSharp.DaoSender</Product>
	  <Description>Auto-generated C# GraphQL client for DaoSender of The-Poolz company.</Description>
	  <PackageProjectUrl>https://github.com/The-Poolz/Poolz.Finance.CSharp.DaoSender</PackageProjectUrl>
	  <PackageReadmeFile>README.md</PackageReadmeFile>
	  <RepositoryUrl>https://github.com/The-Poolz/Poolz.Finance.CSharp.DaoSender</RepositoryUrl>
	  <RepositoryType>git</RepositoryType>
  </PropertyGroup>

	<ItemGroup>
		<None Include="..\..\LICENSE">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
		<None Include="..\..\README.md">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="GraphQL.Client" Version="6.1.0" />
		<PackageReference Include="GraphQL.Client.Serializer.Newtonsoft" Version="6.1.0" />
	</ItemGroup>

</Project>
